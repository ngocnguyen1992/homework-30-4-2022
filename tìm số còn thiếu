#include <stdio.h>
void timso(int arr[], int n) {
    for (int i = 0; i < arr[n-1]; i++) {
        int flag = 1;
        for (int j = 0; j < arr[n-1]; j++) {
            if (arr[j] == i){
                flag = 0;
                break;
            }
        }
        if (flag) {
            printf("%d ", i);
        }
    }
}
void nhapmang(int arr[], int n) {
    printf("Nhap gia tri cho mang : \n");
    for (int i = 0; i < n; i++) {
        scanf_s("%d", &arr[i]);
    }
}
void xuatmang(int arr[], int n) {
    for (int i = 0; i < n; i++)
    {
        printf("%d ", arr[i]);
    }
}
int tangdan(int arr[], int n) {
    int temp;
    for (int i = 0; i < n; i++)
    {
        for (int j = i; j < n; j++) {
            if (arr[i] > arr[j]){
                temp = arr[i];
                arr[i] = arr[j];
                arr[j] = temp;
            }
        }
    }
    return 1;
}
int main()
{
    int n;
    printf("nhap chieu dai cho mang n :");
    scanf_s("%d", &n);
    int arr[1000];
    nhapmang(arr,n);
    printf("mang sau khi sap xep cua ban la :");
    tangdan(arr, n);
    xuatmang(arr, n);
    printf("\ncac so con thieu la :");
    timso(arr, n);
}
